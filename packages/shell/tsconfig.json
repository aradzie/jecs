{
  "extends": "../tsconfig-template.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["es2021", "dom", "dom.iterable"],
    "rootDir": "src",
    "outDir": "lib",
    "incremental": true,
    "tsBuildInfoFile": "lib/.tsbuildcache"
  },
  "include": ["src/**/*"],
  "ts-node": {
    "transpileOnly": true
  }
}
