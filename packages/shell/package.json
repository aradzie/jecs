  "devDependencies": {
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "css-loader": "^6.7.1",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.5.0",
    "monaco-editor-webpack-plugin": "4",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "ava": {
    "extensions": [
      "ts",
